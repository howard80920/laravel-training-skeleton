{
    "compilerOptions": {
        "allowJs": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ESNext",
        "jsx": "preserve",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "isolatedModules": true,
        "strictNullChecks": false,
        "noImplicitAny": false,
        "baseUrl": ".",
        "types": [
            "vite/client",
            "unplugin-icons/types/vue"
        ],
        "paths": {
            "@/*": [
                "clients/*"
            ]
        }
    },
    "include": [
        "clients/**/*.ts",
        "clients/**/*.d.ts",
        "clients/**/*.vue",
    ]
}
